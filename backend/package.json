{
  "name": "express-server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --inspect ./src/server.js --port 8080  --db mongo",
    "dev-sql": "nodemon --inspect ./src/server.js --port 8080  --db sqlite",
    "cluster": "node ./cluster.js 8081",
    "pm2": "pm2 start ./pm2.config.cjs",
    "4ever": "forever start ./src/server.js 8085",
    "start": "node ./src/server.js --db mongo",
    "prof": "node --prof ./src/server.js 8080",
    "artillery": "artillery quick --count 500 -n 200 \"http://localhost:8080/api/info\" > result_artillery.txt",
    "leerProf": "node --prof-process result.log > result-prof.txt",
    "0x": "0x ./src/server.js 8080",
    "test": "node ./src/utils/test.js"
  },
